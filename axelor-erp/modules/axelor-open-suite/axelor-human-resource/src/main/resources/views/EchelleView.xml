<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

	<grid name="echelle-grid_conf" title="echelle" model="com.axelor.apps.configuration.db.Echelle">
		<field name="name"/>
		<field name="grade"/>
		<field name="description"/>
	</grid>

	<form name="echelle-form_conf" title="echelle" model="com.axelor.apps.configuration.db.Echelle">
		<panel name="mainPanel">
			<field name="name"/>
			<field name="grade"/>
			<field name="description" widget="string"/>
		</panel>
	</form>
</object-views>