<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

	<grid name="nature-grid" title="Nature Plainte"
		model="com.axelor.apps.configuration.db.NaturePlainte" orderBy="titre">
		<field name="titre" title="Titre" />
		<field name="description" title="Description" />
	</grid>
	<form name="nature-form" title="Nature Plainte" 
		model="com.axelor.apps.configuration.db.NaturePlainte" width="large" onLoad=""
		onNew="">
			<panel name="employeeDetailsPanel" colSpan="12">
					<field name="titre" title="Titre" />
					<field name="description" title="Description" />
			</panel>
	</form>

</object-views>


	