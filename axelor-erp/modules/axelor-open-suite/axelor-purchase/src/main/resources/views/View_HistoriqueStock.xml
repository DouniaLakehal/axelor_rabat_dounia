<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

<grid name="historiqueStock_grid" title="Historique du stock" model="com.axelor.apps.purchase.db.HistoriqueStock">
	<field name="article"/>
	<field name="typeOperation"/>
	<field name="annexe"/>
	<field name="quantity"/>
	<field name="dateOperation"/>
</grid>

<form name="historiqueStock_form" title="Historique du stock" model="com.axelor.apps.purchase.db.HistoriqueStock" >
	<panel name="historiqueStockPanel" >
		<field name="article" colSpan="6"/>
		<field name="typeOperation" colSpan="6"/>
		<field name="employee" colSpan="6" hidden="true"/>
		<field name="annexe" colSpan="6"/>
		<field name="quantity" colSpan="6"/>
		<field name="dateOperation" colSpan="6"/>
	</panel>
</form>



</object-views>