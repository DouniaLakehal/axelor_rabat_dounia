<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">


    <grid name="assurances-grid" title="Les assurances" model="com.axelor.apps.purchase.db.Assurances">
        <field name="titre" title="Titre"/>
        <field name="piece" widget="binary-link" form-view="meta-files-form" grid-view="meta-files-grid"/>
    </grid>
    <form name="assurances-form" title="Les assurances" model="com.axelor.apps.purchase.db.Assurances">
        <panel name="mainPanel">
                <field name="titre" title="Titre" />
                <field name="piece" title="Document" widget="binary-link" form-view="meta-files-form" grid-view="meta-files-grid" colSpan="6"/>
        </panel>
    </form>
</object-views>
